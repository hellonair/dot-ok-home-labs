while read -r HOST;
do
        echo "$HOST" ;
        USER="admin"
        PASS="admin"
        CMD="cp /etc/passwd /tmp/passwd"

        REMOTEEXEC=$(expect -c "
        spawn ssh $USER@$HOST
        expect \"password:\"
        send \"$PASS\r\"
        expect \"\\\\$\"
        send \"$CMD\r\"
        expect -re \"$USER.*\"
        send \"logout\"
        ")
        echo "$REMOTEEXEC"
done < list_hosts
